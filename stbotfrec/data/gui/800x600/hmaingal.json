                                    "setVariable this/x [this/tmp]",
                                    "setVariable this/tmp [local/tImgOffsetY]",
                                    "modVariable this/tmp [local/components/3/image/height]",
                                    "setVariable this/y [this/tmp]"
                                ]
                            }

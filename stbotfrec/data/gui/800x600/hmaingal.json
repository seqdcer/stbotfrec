                },
                "components": [{
                        "x": 1,
                        "width": 58,
                        "y": 1,
                        "type": "imagebox",
                        "height": 58,
                        "visible": false,
                        "border": {
                            "color": "FFBF8F00",
                            "width": 2,
                            "dashed": false
                        },
                        "triggers": [
                            {
                                "condition": {
                                    "operator": "AND",
                                    "value1": {
                                        "value1": "[runtime/ui/selectedSectorX]",
                                        "value2": "[local/gridX]",
                                        "operator": "=="
                                    },
                                    "value2": {
                                        "value1": "[runtime/ui/selectedSectorY]",
                                        "value2": "[local/gridY]",
                                        "operator": "=="
                                    }
                                },
                                "onTriggered": [
                                    "setVariable this/visible \"true\""
                                ]
                            }, {
                                "condition": {
                                    "operator": "OR",
                                    "value1": {
                                        "value1": "[runtime/ui/selectedSectorX]",
                                        "value2": "[local/gridX]",
                                        "operator": "!="
                                    },
                                    "value2": {
                                        "value1": "[runtime/ui/selectedSectorY]",
                                        "value2": "[local/gridY]",
                                        "operator": "!="
                                    }
                                },
                                "onTriggered": [
                                    "setVariable this/visible \"false\""
                                ]
                            }
                        ]
                    }, {
                        "x": 1,
                        "width": 58,
                        "y": 1,
                        "type": "button",
                        "height": 58,
                        "onClick": [
                            "setVariable runtime/ui/selectedSectorX [local/gridX]",
                            "setVariable runtime/ui/selectedSectorY [local/gridY]"
                        ],
                        "onMouseEntered": [
                            "setVariable runtime/ui/sectorHoverX [local/gridX]",
                            "setVariable runtime/ui/sectorHoverY [local/gridY]"
                        ]
